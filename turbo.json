{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV", "NEXT_TEST_MODE", "PUBLISH"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "out/**",
        "dist/**",
        ".svelte-kit/**",
        ".vercel/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {},
    "typecheck": {
      "outputs": []
    }
  }
}
