{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NODE_ENV", "TEST_MODE"],
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "out/**", "dist/**", ".svelte-kit/**", ".vercel/**", ".output/**", ".nuxt/**", "public/*.js"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "outputs": []
    }
  }
}
