{
	"name": "@serwist-example/sveltekit-basic",
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
