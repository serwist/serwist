    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@serwist/build": "latest",
    "@serwist/svelte": "latest",
    "@sveltejs/adapter-static": "3.0.10",
    "@sveltejs/kit": "2.50.1",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "serwist": "latest",
    "svelte": "5.49.1",
    "svelte-check": "4.3.5",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
