{
  "name": "vite-plugin-serwist",
  "version": "10.0.0-preview.6",
  "type": "module",
  "sideEffects": false,
  "description": "A module that integrates Serwist into your Vite application.",
  "files": [
    "src",
    "dist",
    "*.d.ts"
  ],
  "keywords": [
    "react",
    "remix",
    "vue",
    "preact",
    "svelte",
    "sveltekit",
    "solidjs",
    "vite",
    "vite-plugin",
    "rollup-plugin",
    "serwist",
    "serwistjs",
    "pwa",
    "sw",
    "service worker",
    "web",
    "service-worker"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "antfu <anthonyfu117@hotmail.com>",
  "contributors": [
    "Serwist <ducanh2912.rusty@gmail.com> (https://serwist.pages.dev/)"
  ],
  "license": "MIT",
  "repository": "https://github.com/serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "worker": [
        "./dist/index.worker.d.ts"
      ],
      "schema": [
        "./dist/index.schema.d.ts"
      ],
      "typings": [
        "./typings.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./worker": {
      "types": "./dist/index.worker.d.ts",
      "default": "./dist/index.worker.js"
    },
    "./schema": {
      "types": "./dist/index.schema.d.ts",
      "default": "./dist/index.schema.js"
    },
    "./typings": {
      "types": "./typings.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "qcheck": "biome check ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/utils": "workspace:*",
    "serwist": "workspace:*",
    "glob": "11.0.1",
    "kolorist": "1.8.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@serwist/configs": "workspace:*",
    "@serwist/window": "workspace:*",
    "@types/node": "22.14.1",
    "serwist": "workspace:*",
    "rollup": "4.40.0",
    "typescript": "5.8.3",
    "vite": "6.3.2"
  },
  "peerDependencies": {
    "@serwist/window": "workspace:*",
    "typescript": ">=5.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
