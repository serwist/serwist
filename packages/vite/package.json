{
  "name": "@serwist/vite",
  "version": "9.0.0-preview.6",
  "type": "module",
  "description": "A module that integrates Serwist into your Vite application.",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "react",
    "remix",
    "vue",
    "preact",
    "svelte",
    "sveltekit",
    "solidjs",
    "vite",
    "vite-plugin",
    "serwist",
    "serwistjs",
    "pwa",
    "sw",
    "service worker",
    "web",
    "service-worker"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "antfu <anthonyfu117@hotmail.com>, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/index.browser.d.ts"
      ],
      "worker": [
        "./dist/index.worker.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "default": "./dist/index.browser.js"
    },
    "./worker": {
      "types": "./dist/index.worker.d.ts",
      "default": "./dist/index.worker.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/expiration": "workspace:*",
    "@serwist/strategies": "workspace:*",
    "@serwist/window": "workspace:*",
    "debug": "4.3.4",
    "fast-glob": "3.3.2",
    "pretty-bytes": "6.1.1"
  },
  "devDependencies": {
    "@playwright/test": "1.41.2",
    "@serwist/constants": "workspace:*",
    "@serwist/sw": "workspace:*",
    "@sveltejs/kit": "2.5.0",
    "@types/debug": "4.1.12",
    "@types/node": "20.11.16",
    "@types/prompts": "2.4.9",
    "@types/react": "18.2.54",
    "bumpp": "9.3.0",
    "kolorist": "1.8.0",
    "preact": "10.19.3",
    "prompts": "2.4.2",
    "publint": "0.2.7",
    "react": "18.2.0",
    "rollup": "4.9.6",
    "solid-js": "1.8.14",
    "svelte": "5.0.0-next.48",
    "typescript": "5.4.0-dev.20240206",
    "vite": "5.0.12",
    "vue": "3.4.15"
  },
  "peerDependencies": {
    "@serwist/sw": "workspace:*",
    "@sveltejs/kit": "^1.0.0 || ^2.0.0",
    "preact": "^10.0.0",
    "react": "^18.0.0",
    "solid-js": "^1.8.7",
    "svelte": "^4.0.0 || ^5.0.0",
    "typescript": ">=5.0.0",
    "vite": "^5.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@serwist/sw": {
      "optional": true
    },
    "@sveltejs/kit": {
      "optional": true
    },
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}
