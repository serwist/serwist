{
  "name": "@serwist/vite",
  "version": "9.0.0-preview.18",
  "type": "module",
  "description": "A module that integrates Serwist into your Vite application.",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "react",
    "remix",
    "vue",
    "preact",
    "svelte",
    "sveltekit",
    "solidjs",
    "vite",
    "vite-plugin",
    "serwist",
    "serwistjs",
    "pwa",
    "sw",
    "service worker",
    "web",
    "service-worker"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "antfu <anthonyfu117@hotmail.com>, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/index.browser.d.ts"
      ],
      "worker": [
        "./dist/index.worker.d.ts"
      ],
      "schema": [
        "./dist/index.schema.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./browser": {
      "types": "./dist/index.browser.d.ts",
      "default": "./dist/index.browser.js"
    },
    "./worker": {
      "types": "./dist/index.worker.d.ts",
      "default": "./dist/index.worker.js"
    },
    "./schema": {
      "types": "./dist/index.schema.d.ts",
      "default": "./dist/index.schema.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/sw": "workspace:*",
    "@serwist/window": "workspace:*",
    "glob": "10.3.10",
    "kolorist": "1.8.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "@serwist/sw": "workspace:*",
    "@serwist/utils": "workspace:*",
    "@types/node": "20.11.30",
    "rollup": "4.13.0",
    "typescript": "5.5.0-dev.20240323",
    "vite": "5.2.4"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "vite": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
