{
  "name": "@serwist/build",
  "version": "7.0.0",
  "type": "module",
  "description": "A module that integrates into your build process, helping you generate a manifest of local files that @serwist/sw should precache.",
  "files": [
    "dist",
    "!dist/**/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "caching",
    "fetch requests",
    "offline",
    "file manifest"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Google's Web DevRel Team, DuCanhGH",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "serwist": {
    "packageType": "node_ts"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.module.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.module.d.ts",
        "default": "./dist/index.module.js"
      }
    },
    "./lib/*.js": {
      "require": {
        "types": "./dist/lib/*.d.cts",
        "default": "./dist/lib/*.cjs"
      },
      "default": {
        "types": "./dist/lib/*.module.d.ts",
        "default": "./dist/lib/*.module.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production rollup --config rollup.config.js"
  },
  "dependencies": {
    "@apideck/better-ajv-errors": "0.3.6",
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@babel/runtime": "7.23.2",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.4",
    "@rollup/plugin-swc": "0.2.1",
    "@rollup/plugin-terser": "0.4.4",
    "@serwist/background-sync": "workspace:*",
    "@serwist/broadcast-update": "workspace:*",
    "@serwist/cacheable-response": "workspace:*",
    "@serwist/core": "workspace:*",
    "@serwist/expiration": "workspace:*",
    "@serwist/google-analytics": "workspace:*",
    "@serwist/navigation-preload": "workspace:*",
    "@serwist/precaching": "workspace:*",
    "@serwist/range-requests": "workspace:*",
    "@serwist/recipes": "workspace:*",
    "@serwist/routing": "workspace:*",
    "@serwist/strategies": "workspace:*",
    "@serwist/streams": "workspace:*",
    "@serwist/sw": "workspace:*",
    "@serwist/window": "workspace:*",
    "@swc/core": "1.3.93",
    "ajv": "8.12.0",
    "common-tags": "1.8.2",
    "fast-json-stable-stringify": "2.1.0",
    "fs-extra": "11.1.1",
    "fsevents": "2.3.3",
    "glob": "10.3.10",
    "lodash": "4.17.21",
    "pretty-bytes": "6.1.1",
    "rollup": "3.28.1",
    "source-map": "0.8.0-beta.0",
    "stringify-object": "5.0.0",
    "strip-comments": "2.0.1",
    "tempy": "3.1.0",
    "upath": "2.0.1"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "@types/babel__preset-env": "7.9.4",
    "@types/common-tags": "1.8.3",
    "@types/fs-extra": "11.0.2",
    "@types/lodash": "4.14.200",
    "@types/node": "20.8.7",
    "@types/stringify-object": "4.0.3",
    "@types/strip-comments": "2.0.3",
    "@types/surma__rollup-plugin-off-main-thread": "2.2.2",
    "type-fest": "4.4.0"
  }
}
