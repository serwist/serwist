{
  "name": "@serwist/build",
  "version": "8.0.3",
  "type": "module",
  "description": "A module that integrates into your build process, helping you generate a manifest of local files that should be precached.",
  "files": [
    "dist",
    "!dist/**/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "caching",
    "fetch requests",
    "offline",
    "file manifest"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Google's Web DevRel Team, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "module": "./dist/index.js",
  "main": "./dist/index.old.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.old.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_OPTIONS='--max-old-space-size=4096' NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@apideck/better-ajv-errors": "0.3.6",
    "@serwist/background-sync": "workspace:*",
    "@serwist/broadcast-update": "workspace:*",
    "@serwist/cacheable-response": "workspace:*",
    "@serwist/core": "workspace:*",
    "@serwist/expiration": "workspace:*",
    "@serwist/google-analytics": "workspace:*",
    "@serwist/precaching": "workspace:*",
    "@serwist/routing": "workspace:*",
    "ajv": "8.12.0",
    "common-tags": "1.8.2",
    "fast-json-stable-stringify": "2.1.0",
    "fs-extra": "11.1.1",
    "glob": "10.3.10",
    "pretty-bytes": "6.1.1",
    "rollup": "3.28.1",
    "source-map": "0.8.0-beta.0",
    "stringify-object": "5.0.0",
    "strip-comments": "2.0.1",
    "upath": "2.0.1"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "@types/common-tags": "1.8.3",
    "@types/fs-extra": "11.0.2",
    "@types/node": "20.8.7",
    "@types/stringify-object": "4.0.3",
    "@types/strip-comments": "2.0.3",
    "type-fest": "4.4.0"
  }
}
