{
  "name": "@serwist/core",
  "version": "8.1.0",
  "type": "module",
  "description": "This module is used by a number of the other Serwist modules to share common code.",
  "files": [
    "dist",
    "!dist/**/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "sw"
  ],
  "author": "Google's Web DevRel Team, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "internal": [
        "./dist/index.internal.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.old.d.cts",
        "default": "./dist/index.old.cjs"
      }
    },
    "./internal": {
      "import": {
        "types": "./dist/index.internal.d.ts",
        "default": "./dist/index.internal.js"
      },
      "require": {
        "types": "./dist/index.internal.old.d.cts",
        "default": "./dist/index.internal.old.cjs"
      }
    }
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "rollup": "4.9.1"
  }
}
