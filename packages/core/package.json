{
  "name": "@serwist/core",
  "version": "7.0.0",
  "type": "module",
  "description": "This module is used by a number of the other Serwist modules to share common code.",
  "files": [
    "dist",
    "!dist/**/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "sw"
  ],
  "author": "Google's Web DevRel Team, DuCanhGH",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "serwist": {
    "browserNamespace": "serwist.core",
    "packageType": "sw"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "types": ["./dist/types.d.ts"]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": null
    }
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "rollup": "3.28.1"
  }
}
