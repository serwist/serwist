{
  "name": "@serwist/cli",
  "version": "9.4.0",
  "type": "module",
  "description": "The command line interface of Serwist.",
  "files": [
    "dist",
    "cli.js"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "caching",
    "fetch requests",
    "offline",
    "cli"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Google's Web DevRel Team",
  "contributors": [
    "Serwist <ducanh2912.rusty@gmail.com> (https://serwist.pages.dev/)"
  ],
  "license": "MIT",
  "repository": "https://github.com/serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/index.schema.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/index.schema.d.ts",
      "default": "./dist/index.schema.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "serwist": "cli.js"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@inquirer/prompts": "8.1.0",
    "@serwist/build": "workspace:*",
    "chalk": "5.6.2",
    "chokidar": "5.0.0",
    "common-tags": "1.8.2",
    "glob": "10.5.0",
    "meow": "14.0.0",
    "pretty-bytes": "6.1.1",
    "stringify-object": "6.0.0",
    "update-notifier": "7.3.1",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@serwist/configs": "workspace:*",
    "@serwist/utils": "workspace:*",
    "@types/common-tags": "1.8.4",
    "@types/inquirer": "9.0.9",
    "@types/stringify-object": "4.0.5",
    "@types/update-notifier": "6.0.8",
    "esbuild": "0.27.2",
    "rollup": "4.54.0",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "esbuild": ">=0.25.0 <1.0.0"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    }
  }
}
