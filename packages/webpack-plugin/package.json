{
  "name": "@serwist/webpack-plugin",
  "version": "7.0.0",
  "type": "module",
  "description": "A plugin for your Webpack build process, helping you generate a manifest of local files that @serwist/sw should precache.",
  "keywords": [
    "serwist",
    "serwistjs",
    "webpack",
    "service worker",
    "caching",
    "fetch requests",
    "offline",
    "file manifest"
  ],
  "serwist": {
    "packageType": "node_ts"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "fast-json-stable-stringify": "2.1.0",
    "pretty-bytes": "6.1.1",
    "upath": "2.0.1",
    "webpack-sources": "3.2.3"
  },
  "peerDependencies": {
    "webpack": "4.4.0 || ^5.9.0"
  },
  "devDependencies": {
    "@types/node": "20.8.7",
    "@types/webpack": "5.28.3"
  },
  "author": "Google's Web DevRel Team, DuCanhGH",
  "license": "MIT",
  "repository": "DuCanhGH/next-pwa",
  "bugs": "https://github.com/DuCanhGH/next-pwa/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app"
}
