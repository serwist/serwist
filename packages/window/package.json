{
  "name": "@serwist/window",
  "version": "8.0.3",
  "type": "module",
  "description": "Simplifies communications with Serwist packages running in the service worker",
  "files": [
    "dist",
    "!dist/**/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "sw",
    "window",
    "message",
    "postMessage"
  ],
  "author": "Google's Web DevRel Team, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "module": "./dist/index.js",
  "main": "./dist/index.old.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.old.d.cts",
        "default": "./dist/index.old.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/core": "workspace:*",
    "@types/trusted-types": "2.0.7"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "rollup": "3.28.1"
  }
}
