{
  "name": "@serwist/window",
  "version": "7.0.0",
  "type": "module",
  "description": "Simplifies communications with Serwist packages running in the service worker",
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "sw",
    "window",
    "message",
    "postMessage"
  ],
  "author": "Google's Web DevRel Team, DuCanhGH",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://ducanh-next-pwa.vercel.app",
  "serwist": {
    "packageType": "window",
    "primaryBuild": "dist/index.module.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.module.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.module.d.ts",
        "default": "./dist/index.module.js"
      }
    }
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js"
  },
  "dependencies": {
    "@serwist/core": "7.0.0",
    "@types/trusted-types": "2.0.4"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "rollup": "3.28.1"
  }
}
