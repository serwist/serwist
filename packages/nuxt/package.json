{
  "name": "@serwist/nuxt",
  "version": "10.0.0-preview.8",
  "type": "module",
  "sideEffects": false,
  "description": "A Nuxt module that integrates Serwist into your application.",
  "files": [
    "dist"
  ],
  "keywords": [
    "nuxt",
    "nuxt-module",
    "pwa",
    "vite",
    "vite-plugin",
    "serwist",
    "serwistjs",
    "pwa",
    "sw",
    "service worker",
    "web",
    "service-worker"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "repository": "https://github.com/serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "main": "./dist/module.mjs",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "build": {
    "externals": [
      "node:child_process",
      "node:fs",
      "node:path",
      "consola",
      "esbuild",
      "rollup",
      "vite",
      "@nuxt/kit",
      "@serwist/build",
      "vite-plugin-serwist",
      "virtual:serwist-nuxt-configuration",
      "virtual:serwist"
    ]
  },
  "scripts": {
    "build": "nuxt-module-build prepare && nuxt-module-build build",
    "dev": "nuxi dev playground --port 5173",
    "dev:generate": "nuxi generate playground",
    "dev:generate:netlify": "NITRO_PRESET=netlify nuxi generate playground",
    "dev:generate:vercel": "NITRO_PRESET=vercel nuxi generate playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "dev:preview:build": "nr dev:build && node playground/.output/server/index.mjs",
    "dev:preview:generate": "nr dev:generate && serve playground/dist"
  },
  "dependencies": {
    "@nuxt/kit": "3.16.2",
    "@serwist/build": "workspace:*",
    "@serwist/utils": "workspace:*",
    "@serwist/window": "workspace:*",
    "vite-plugin-serwist": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "1.0.1",
    "@nuxt/schema": "3.16.2",
    "@serwist/configs": "workspace:*",
    "@types/node": "22.15.2",
    "eslint": "9.25.1",
    "nuxt": "3.16.2",
    "rimraf": "6.0.1",
    "serve": "14.2.4",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vue-tsc": "2.2.10"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0",
    "vite": "^5.0.0 || ^6.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
