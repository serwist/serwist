{
  "name": "@serwist/sw",
  "version": "9.0.0-preview.15",
  "type": "module",
  "description": "A module that makes it easy to get started with the Serwist service worker libraries.",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "service worker",
    "sw"
  ],
  "author": "Google's Web DevRel Team, Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "plugins": [
        "./dist/index.plugins.d.ts"
      ],
      "strategies": [
        "./dist/index.strategies.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/index.plugins.d.ts",
      "default": "./dist/index.plugins.js"
    },
    "./strategies": {
      "types": "./dist/index.strategies.d.ts",
      "default": "./dist/index.strategies.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/background-sync": "workspace:*",
    "@serwist/broadcast-update": "workspace:*",
    "@serwist/cacheable-response": "workspace:*",
    "@serwist/core": "workspace:*",
    "@serwist/expiration": "workspace:*",
    "@serwist/google-analytics": "workspace:*",
    "@serwist/navigation-preload": "workspace:*",
    "@serwist/precaching": "workspace:*",
    "@serwist/range-requests": "workspace:*",
    "@serwist/routing": "workspace:*",
    "@serwist/strategies": "workspace:*"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "@serwist/utils": "workspace:*",
    "rollup": "4.13.0",
    "typescript": "5.5.0-dev.20240323"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
