{
  "name": "@serwist/svelte",
  "version": "9.0.4",
  "type": "module",
  "description": "A module that complements SvelteKit's built-in service worker support.",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "sw",
    "service worker",
    "progressive web apps",
    "web",
    "service-worker",
    "progressive-web-apps",
    "svelte",
    "sveltekit",
    "pwa"
  ],
  "author": "Serwist's Team",
  "license": "MIT",
  "repository": "https://github.com/serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "typesVersions": {
    "*": {
      "worker": [
        "./dist/index.worker.d.ts"
      ]
    }
  },
  "exports": {
    "./worker": {
      "types": "./dist/index.worker.d.ts",
      "default": "./dist/index.worker.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && NODE_ENV=production rollup --config rollup.config.js",
    "dev": "rollup --config rollup.config.js --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "serwist": "workspace:*"
  },
  "devDependencies": {
    "@serwist/configs": "workspace:*",
    "@sveltejs/kit": "2.5.18",
    "rollup": "4.18.1"
  },
  "peerDependencies": {
    "@sveltejs/kit": ">=2.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
