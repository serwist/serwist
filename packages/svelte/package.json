{
  "name": "@serwist/svelte",
  "version": "9.5.1",
  "type": "module",
  "description": "A module that complements SvelteKit's built-in service worker support.",
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "sw",
    "service worker",
    "progressive web apps",
    "web",
    "service-worker",
    "progressive-web-apps",
    "svelte",
    "sveltekit",
    "pwa"
  ],
  "author": "Serwist <ducanh2912.rusty@gmail.com> (https://serwist.pages.dev/)",
  "license": "MIT",
  "repository": "https://github.com/serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "./dist/index.svelte.d.ts"
      ],
      "worker": [
        "./dist/index.worker.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/index.svelte.d.ts",
      "svelte": "./dist/index.svelte.js"
    },
    "./worker": {
      "types": "./dist/index.worker.d.ts",
      "default": "./dist/index.worker.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rimraf dist && svelte-package",
    "dev": "svelte-package --watch",
    "lint": "biome lint ./src",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/utils": "workspace:*",
    "@serwist/window": "workspace:*",
    "@sveltejs/package": "2.5.7",
    "esm-env": "1.2.2",
    "serwist": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/kit": "2.50.1",
    "svelte": "5.49.1"
  },
  "peerDependencies": {
    "@sveltejs/kit": ">=2.0.0",
    "svelte": ">=5.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  }
}
