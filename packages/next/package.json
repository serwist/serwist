{
  "name": "@serwist/next",
  "version": "7.0.0",
  "type": "module",
  "description": "PWA for Next.js, powered by Serwist.",
  "repository": "https://github.com/serwist/serwist",
  "author": "ShadowWalker <w@weiw.io>, DuCanhGH",
  "license": "MIT",
  "private": false,
  "files": [
    "dist",
    "!dist/dts"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.old.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.old.cjs",
      "types": "./dist/index.d.ts"
    },
    "./typings": {
      "import": "./dist/sw-entry.d.ts",
      "require": "./dist/sw-entry.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "typings": [
        "./dist/sw-entry.d.ts"
      ]
    }
  },
  "keywords": [
    "nextjs",
    "pwa",
    "serwist",
    "web",
    "service-worker"
  ],
  "scripts": {
    "dev": "rimraf dist && rollup --config --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config --compact",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/core": "workspace:*",
    "@serwist/webpack-plugin": "workspace:*",
    "@serwist/window": "workspace:*",
    "clean-webpack-plugin": "4.0.0",
    "fast-glob": "3.3.1",
    "semver": "7.5.4",
    "terser-webpack-plugin": "5.3.9"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "5.0.1",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-swc": "0.2.1",
    "@rollup/plugin-typescript": "11.1.5",
    "@serwist/constants": "workspace:*",
    "@swc/core": "1.3.93",
    "@types/node": "20.8.7",
    "@types/semver": "7.5.3",
    "chalk": "5.3.0",
    "next": "13.5.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.28.1",
    "rollup-plugin-dts": "6.0.2",
    "type-fest": "4.5.0",
    "typescript": "5.3.0-dev.20231011",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "next": ">=11.0.0",
    "webpack": ">=5.9.0"
  }
}
