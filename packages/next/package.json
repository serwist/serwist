{
  "name": "@serwist/next",
  "version": "8.3.0",
  "type": "module",
  "description": "A module that integrates Serwist into your Next.js application.",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "keywords": [
    "serwist",
    "serwistjs",
    "sw",
    "service worker",
    "web",
    "service-worker"
  ],
  "author": "Serwist's Team",
  "license": "MIT",
  "repository": "serwist/serwist",
  "bugs": "https://github.com/serwist/serwist/issues",
  "homepage": "https://serwist.pages.dev",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./browser": {
      "import": {
        "types": "./dist/index.browser.d.ts",
        "default": "./dist/index.browser.js"
      },
      "require": {
        "types": "./dist/index.browser.d.cts",
        "default": "./dist/index.browser.cjs"
      }
    },
    "./typings": {
      "import": {
        "types": "./dist/sw-entry.d.ts"
      },
      "require": {
        "types": "./dist/sw-entry.d.cts"
      }
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "browser": [
        "./dist/index.browser.d.ts"
      ],
      "typings": [
        "./dist/sw-entry.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@serwist/build": "workspace:*",
    "@serwist/webpack-plugin": "workspace:*",
    "@serwist/window": "workspace:*",
    "clean-webpack-plugin": "4.0.0",
    "fast-glob": "3.3.2"
  },
  "devDependencies": {
    "@serwist/constants": "workspace:*",
    "@types/node": "20.10.5",
    "chalk": "5.3.0",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "4.9.1",
    "type-fest": "4.8.3",
    "typescript": "5.4.0-dev.20231226",
    "webpack": "5.89.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "webpack": ">=5.9.0"
  }
}
